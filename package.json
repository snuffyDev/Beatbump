{
	"name": "beatbump",
	"version": "0.1.5",
	"scripts": {
		"dev": "svelte-kit dev --port 5000 --host",
		"deploy": "npm run build && wrangler publish",
		"deploy:clean": "npm run cache:delete && npm run build && wrangler publish",
		"cache:delete": "rmdir /S /Q build && rmdir /S /Q .svelte-kit",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview --host",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@babel/core": "^7.14.0",
		"@snuffydev/adapter-cloudflare-cache": "^1.0.16",
		"@sveltejs/adapter-cloudflare-workers": "next",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^4.28.3",
		"@typescript-eslint/parser": "^4.28.3",
		"autoprefixer": "^10.2.5",
		"babel-loader": "^8.2.2",
		"cssnano": "^4.1.10",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.1.0",
		"peerjs": "^1.3.2",
		"postcss": "^8.2.7",
		"postcss-load-config": "^3.0.1",
		"postcss-uncss": "^0.17.0",
		"prettier": "~2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"sass": "^1.35.2",
		"svelte": "^3.38.2",
		"svelte-check": "^1.5.1",
		"svelte-loader": "^3.1.1",
		"svelte-preprocess": "^4.7.4",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5",
		"vite": "^2.1.0",
		"vite-plugin-iso-import": "^0.1.1"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	},
	"dependencies": {
		"@fontsource/commissioner": "^4.5.0",
		"cookie": "^0.4.1"
	}
}
